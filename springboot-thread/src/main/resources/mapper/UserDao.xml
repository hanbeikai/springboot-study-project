<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.beikai.springbootthread.mapper.UserDao" >
    <insert id="addUser" parameterType="com.beikai.springbootthread.model.User">
        insert into thread_user(name) values(#{name})
    </insert>

    <select id="getUser" resultType="com.beikai.springbootthread.model.User">
        select * from thread_user
    </select>
</mapper>